<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home.integral.IntegralActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="215dp"
        android:background="#FF000000 "
        >
        <ImageView
            android:id="@+id/back"
            android:layout_marginTop="14.5dp"
            android:layout_marginLeft="11.5dp"
            android:src="@drawable/back_white"
            android:layout_width="25dp"
            android:layout_height="25dp"/>
        <TextView
            android:id="@+id/jifen"
            android:text="@string/jifen"
            android:layout_marginTop="18.5dp"
            android:textSize="18sp"
            android:textColor="#FFFFFFFF "
            android:layout_centerHorizontal="true"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <TextView
            android:id="@+id/about_jifen"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/about_jifen"
            android:layout_alignParentRight="true"
            android:textColor="#FFFFFFFF"
            android:layout_marginTop="20dp"
            android:textSize="15sp"
            android:layout_marginRight="15dp"
            />
        <TextView
            android:id="@+id/jifen_title"
            android:layout_below="@+id/jifen"
            android:layout_centerHorizontal="true"
            android:textSize="54sp"
            android:text="666"
            android:textColor="#FFFFFFFF "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:textStyle="bold"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/jifen_title"
            android:text="@string/use_jifen"
            android:layout_marginTop="17dp"
            android:layout_centerHorizontal="true"
            android:textColor="#FFFFFFFF"
            android:textSize="15sp"
            />

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#fff4f4f4" />

    <com.flyco.roundview.RoundLinearLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:clickable="true"
        android:gravity="center_vertical"
        app:rv_backgroundColor="@color/white"
        app:rv_backgroundPressColor="#fff4f4f4">
        <TextView
            android:id="@+id/renwu"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="15dp"
            android:text="普通任务"
            android:textSize="16sp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#FF282B2E " />


    </com.flyco.roundview.RoundLinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="#ffd6d8da" />

    <com.flyco.roundview.RoundLinearLayout
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:clickable="true"
        app:rv_backgroundColor="@color/white"
        app:rv_backgroundPressColor="#fff4f4f4">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="75dp">
            <TextView
                android:id="@+id/register"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_marginLeft="15dp"
                android:text="注册成功"
                android:layout_marginTop="15dp"
                android:textSize="16sp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#FF282B2E " />
            <ImageView
                android:id="@+id/money_register"
                android:layout_toRightOf="@id/register"
                android:src="@drawable/money"
                android:paddingLeft="10dp"
                android:layout_marginTop="17dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_toRightOf="@+id/money_register"
                android:layout_marginTop="15.5dp"
                android:layout_marginLeft="10dp"
                android:text="+100"
                android:textColor="#FFE64A56 "
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="成功认证，可获得1次积分"
                android:layout_below="@+id/register"
                android:layout_marginTop="8.5dp"
                android:layout_marginLeft="15dp"
                />
            <TextView
                android:text="已完成"
                android:layout_marginRight="15dp"
                android:layout_alignParentRight="true"
                android:layout_marginTop="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RelativeLayout>

    </com.flyco.roundview.RoundLinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#fff4f4f4" />
    <com.flyco.roundview.RoundLinearLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:clickable="true"
        android:gravity="center_vertical"
        app:rv_backgroundColor="@color/white"
        app:rv_backgroundPressColor="#fff4f4f4">
        <TextView
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="15dp"
            android:text="每日任务"
            android:textSize="16sp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#FF282B2E " />


    </com.flyco.roundview.RoundLinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="#ffd6d8da" />
    <com.flyco.roundview.RoundLinearLayout
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:clickable="true"
        app:rv_backgroundColor="@color/white"
        app:rv_backgroundPressColor="#fff4f4f4">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="75dp">
            <TextView
                android:id="@+id/qiandao"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_marginLeft="15dp"
                android:text="每日签到"
                android:layout_marginTop="15.5dp"
                android:textSize="16sp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#FF282B2E " />

            <ImageView
                android:id="@+id/money_qiandao"
                android:layout_toRightOf="@id/qiandao"
                android:src="@drawable/money"
                android:paddingLeft="10dp"
                android:layout_marginTop="17dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_toRightOf="@+id/money_qiandao"
                android:layout_marginTop="15.5dp"
                android:layout_marginLeft="10dp"
                android:text="+20"
                android:textColor="#FFE64A56 "
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="每日登录可获得1次积分"
                android:layout_below="@+id/qiandao"
                android:layout_marginTop="8.5dp"
                android:layout_marginLeft="15dp"
                />

            <TextView
                android:id="@+id/qiandao_tv"
                android:text="未签到"
                android:layout_marginRight="15dp"
                android:layout_alignParentRight="true"
                android:layout_marginTop="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RelativeLayout>

    </com.flyco.roundview.RoundLinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="#ffd6d8da" />
    <com.flyco.roundview.RoundLinearLayout
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:clickable="true"
        app:rv_backgroundColor="@color/white"
        app:rv_backgroundPressColor="#fff4f4f4">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="75dp">
            <TextView
                android:id="@+id/read"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_marginLeft="15dp"
                android:text="阅读文章"
                android:layout_marginTop="15.5dp"
                android:textSize="16sp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#FF282B2E " />

            <ImageView
                android:id="@+id/money_read"
                android:layout_toRightOf="@id/read"
                android:src="@drawable/money"
                android:paddingLeft="10dp"
                android:layout_marginTop="17dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_toRightOf="@+id/money_read"
                android:layout_marginTop="15.5dp"
                android:layout_marginLeft="10dp"
                android:text="+10"
                android:textColor="#FFE64A56 "
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="每日最多可获得100积分"
                android:layout_below="@+id/read"
                android:layout_marginTop="8.5dp"
                android:layout_marginLeft="15dp"
                />
            <TextView
                android:textColor="#FF282B2E"
                android:textSize="15sp"
                android:id="@+id/ten"
                android:text="/10"
                android:layout_marginRight="15.5dp"
               android:layout_marginTop="15.5dp"
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:textColor="#FF282B2E"
                android:textSize="15sp"
                android:id="@+id/ten_tv"
                android:text="0"
                android:layout_marginTop="15.5dp"
                android:layout_toLeftOf="@+id/ten"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RelativeLayout>

    </com.flyco.roundview.RoundLinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="#ffd6d8da" />
    <com.flyco.roundview.RoundLinearLayout
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:clickable="true"
        app:rv_backgroundColor="@color/white"
        app:rv_backgroundPressColor="#fff4f4f4">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="75dp">
            <TextView
                android:id="@+id/share"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_marginLeft="15dp"
                android:text="分享文章"
                android:layout_marginTop="15.5dp"
                android:textSize="16sp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#FF282B2E " />
            <ImageView
                android:id="@+id/money_share"
                android:layout_toRightOf="@id/share"
                android:src="@drawable/money"
                android:paddingLeft="10dp"
                android:layout_marginTop="17dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_toRightOf="@+id/money_share"
                android:layout_marginTop="15.5dp"
                android:layout_marginLeft="10dp"
                android:text="+20"
                android:textColor="#FFE64A56 "
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="每日最多可获得200积分"
                android:layout_below="@+id/share"
                android:layout_marginTop="8.5dp"
                android:layout_marginLeft="15dp"
                />

            <TextView
                android:textColor="#FF282B2E"
                android:textSize="15sp"
                android:id="@+id/ten1"
                android:text="/10"
                android:layout_marginRight="15.5dp"
                android:layout_marginTop="15.5dp"
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:textColor="#FF282B2E"
                android:textSize="15sp"
                android:id="@+id/ten1_tv"
                android:text="0"
                android:layout_marginTop="15.5dp"
                android:layout_toLeftOf="@+id/ten1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RelativeLayout>

    </com.flyco.roundview.RoundLinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#fff4f4f4" />




</androidx.appcompat.widget.LinearLayoutCompat>